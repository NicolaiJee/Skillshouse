
@{
    Layout = null;
}

<!--
1. Add 4 divs to the body
2. Rotate div 1
3. Translate div 2
4. Scale div 3
5. skew div 4

-->
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Demo2End</title>
    <style>
        #div1 {
  transform: rotate(10deg);
  transform-origin: left top;
            border: 1px solid blue;
} 
        #div2 {
  transform: translateX(50%);
  transform-origin: left top;
            border: 1px solid blue;
} 
        #div3 {
  transform: scaleX(2);
  transform-origin: left top;
            border: 1px solid blue;
} 
        #div4 {
  transform: skewX(30deg);
  transform-origin: left top;
            border: 1px solid blue;
} 

    </style>
</head>
<body>
<div id="div1">
    Div 1
</div><br /><br /><br />
    <div id="div2">        Div 2
    </div><br /><br /><br />
    <div id="div3">        Div 3
    </div><br /><br /><br />
    <div id="div4">        Div 4
    </div>
</body>
</html>
